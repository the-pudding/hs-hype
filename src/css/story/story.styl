/* CUSTOM STORY STYLE */
.chart__flow
  max-width $column-width
  margin 0 auto
  height 550px
  svg
    background-color: #F5F2E7
    path
      stroke-width 1px
      fill none
    .g-labels
      .label
          font-family $national
          text-transform: uppercase;
          fill #6B361A
          font-weight bold
